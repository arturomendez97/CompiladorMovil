//////////////T(TERMINO)/////////////////
Token T(Token func) :
{
    Token var;
}
{
    var = F(func)
    T2(func)
    {return var;}
}

void T2(Token func) :
{}
{
        <MULT>T(func)
    |   <DIV>T(func)
    |   Empty()
