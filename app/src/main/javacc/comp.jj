//////////////F(FACTOR)/////////////////
Token F(Token func) :
{
    Token var;
}
{
        <PARENIZQ>
        var = Expresion(func)<PARENDER>
        {return var;}
    |   var = Variable()
        {return var;}
    //|   var = Llamada(func)
    //    {return var;}
    |   var = F2()
        {return var;}
}

Token F2() :
{
    Token var;
}
{
        var = <CTEI>
        {return var;}
    |   var = <CTEF>
        {return var;}
    |   var = <ID>
        {return var;}
}


